{
    "name": "e3-development-solutions/laravel-quickbooks-integration",
    "description": "A Laravel package for seamless QuickBooks Online integration with OAuth middleware and scaffolding commands",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "quickbooks",
        "oauth",
        "integration",
        "accounting",
        "intuit"
    ],
    "authors": [
        {
            "name": "E3 Development Solutions",
            "email": "info@e3development.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "quickbooks/v3-php-sdk": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "E3DevelopmentSolutions\\LaravelQuickBooksIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "E3DevelopmentSolutions\\LaravelQuickBooksIntegration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "E3DevelopmentSolutions\\LaravelQuickBooksIntegration\\Providers\\QuickBooksIntegrationServiceProvider"
            ],
            "aliases": {
                "QuickBooks": "E3DevelopmentSolutions\\LaravelQuickBooksIntegration\\Facades\\QuickBooks"
            }
        }
    },
  "minimum-stability": "beta",
    "prefer-stable": false,
    "config": {
        "sort-packages": true
    }
}

